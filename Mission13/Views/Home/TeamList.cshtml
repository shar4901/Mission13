@{
    ViewData["Title"] = "Team List";
}

@model List<Team>


<table class="table table-striped table-bordered">
    <tr>

        <th>Team Name</th>
        <th>Team Id</th>
    </tr>
    <tbody>
        @foreach (var team in Model)
        {
        <tr>
            <td>@team.TeamName</td>
            <td>@team.TeamId</td>

        </tr>
        }
    </tbody>
</table>

